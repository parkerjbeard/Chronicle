<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- SECURE ENTITLEMENTS FOR CHRONICLE UI -->
	<!-- Minimal permissions for UI functionality only -->
	
	<!-- Enable app sandbox for security -->
	<key>com.apple.security.app-sandbox</key>
	<true/>
	
	<!-- Network client for API communication only -->
	<key>com.apple.security.network.client</key>
	<true/>
	
	<!-- File access limited to user-selected files -->
	<key>com.apple.security.files.user-selected.read-write</key>
	<true/>
	
	<!-- Downloads folder for export functionality -->
	<key>com.apple.security.files.downloads.read-write</key>
	<true/>
	
	<!-- REMOVED DANGEROUS ENTITLEMENTS:
	     - Disabled app sandbox (com.apple.security.app-sandbox = false)
	     - Network server capabilities
	     - Location access
	     - Camera/microphone access
	     - Automation/Apple Events access
	-->
</dict>
</plist>